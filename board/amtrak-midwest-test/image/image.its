/dts-v1/;

/ {
	description = "Test FIT image";
	#address-cells = <1>;

	images {
		kernel {
			description = "default kernel";
			data = /incbin/("./zImage");
			type = "kernel";
			arch = "arm";
			os = "linux";
			compression = "none";
			load = <0x82000000>;
			entry = <0x82000000>;
			hash@1 {
				algo = "sha1";
			};
		};

		ramdisk {
			description = "rootfs";
			data = /incbin/("./rootfs.cpio.gz");
			type = "ramdisk";
			arch = "arm";
			os = "linux";
			hash@1 {
				algo = "sha1";
			};
		};

		fdt {
			description = "device tree";
			data = /incbin/("./allwinner/amtrak-midwest-test.dtb");
			type = "flat_dt";
			arch = "arm";
			compression = "none";
			load = <0x81EE0000>;
			entry = <0x81EE0000>;
			hash@1 {
				algo = "sha1";
			};
		};

	};

	configurations {
		default = "amtrak-tracker";

		amtrak-tracker {
			description = "amtrak tracker default configuration";
			kernel = "kernel";
			ramdisk = "ramdisk";
			fdt = "fdt";
		};
	};
};
